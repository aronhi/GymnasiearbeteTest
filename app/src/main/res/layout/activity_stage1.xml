<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_stage1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.ah980627.gymnasiearbetetest.Stage1Activity">

    <TextView
        android:text="Box1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="131dp"
        android:id="@+id/box1"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Box2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/box1"
        android:layout_toRightOf="@+id/box1"
        android:layout_toEndOf="@+id/box1"
        android:layout_marginLeft="59dp"
        android:layout_marginStart="59dp"
        android:id="@+id/box2" />

    <TextView
        android:text="box3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/box2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="48dp"
        android:layout_marginEnd="48dp"
        android:id="@+id/box3" />

    <TextView
        android:text="box4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/box1"
        android:layout_toEndOf="@+id/box1"
        android:layout_marginBottom="54dp"
        android:id="@+id/box4" />

    <TextView
        android:text="box5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/box4"
        android:layout_toLeftOf="@+id/box3"
        android:layout_toStartOf="@+id/box3"
        android:layout_marginRight="23dp"
        android:layout_marginEnd="23dp"
        android:id="@+id/box5" />
</RelativeLayout>
